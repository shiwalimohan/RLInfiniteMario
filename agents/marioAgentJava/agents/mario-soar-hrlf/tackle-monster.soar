sp {propose*tackle-monster1
   (state <s> ^name mario-soar-hrlf
              ^monster <m>
            - ^coin.isreachable yes
            - ^question.isreachable yes)
   (<m> ^isthreat yes)
-->
   (<s> ^operator <o> +)
   (<o> ^name tackle-monster
        ^monster <m>)
}
