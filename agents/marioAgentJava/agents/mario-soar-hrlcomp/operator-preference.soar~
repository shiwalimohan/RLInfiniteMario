sp {set-pref*question*coin
   (state <s> ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name search-question)
   (<o2> ^name grab-coin)
-->
   (<s> ^operator <o1> > <o2>)
}
sp {set-pref*monster*question
   (state <s> ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name tackle-monster)
   (<o2> ^name search-question)
-->
   (<s> ^operator <o1> > <o2>)
}
sp {set-pref*monster*coin
   (state <s> ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name tackle-monster)
   (<o2> ^name grab-coin)
-->
   (<s> ^operator <o1> > <o2>)
}
sp {set-pref*monster*pit1
   (state <s> ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name tackle-monster
         ^monster.ditx <dx> > 0)
   (<o2> ^name avoid-pit
         ^pit.dist-start > <dx>)
-->
   (<s> ^operator <o1> > <o2>)
}
sp {set-pref*monster*pit2
   (state <s> ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name tackle-monster
         ^monster.distx <dx> < 0)
   (<o2> ^name avoid-pit
         ^pit.dist-start < <dx>)
-->
   (<s> ^operator <o1> > <o2>)
}