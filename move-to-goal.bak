sp {propose*move-to-goal
   (state <s> ^name mario-soar-hrl)
-->
   (<s> ^operator <o> + <)
   (<o> ^name move-to-goal)
}
sp {move-to-goal*random*action1
   (state <s> ^name move-to-goal
              ^rand-jump <jp>
              ^rand-speed <sp>
              ^height <h>
              ^mario <mario>)
   (<h> ^distx <dx>
        ^height <height>
        ^isreachable yes)
   (<jp> ^up <j>) 
   (<mario> ^xi <mx>
            ^yi <my>)
   (<sp> ^degree <sd>)
-->
   (<s> ^operator <o> +)
   (<o> ^name action
        ^direction right
        ^jump <j>
        ^speed <sp>)
}

sp {move*to*goal1
   :template
   (state <s> ^name move-to-goal
              ^operator <op> +
              ^height <h>
              ^mario <mario>)
   (<mario> ^type <mariot>
        ^tile-above <ta>)
   (<op> ^name action
         ^direction <d>
         ^jump <j>
         ^speed <sp>)
   (<h> ^distx <dx>
        ^height <height>
        ^isreachable yes)
--> 
   (<s> ^operator <op> = 0)
}

sp {move-to-goal*random*action2
   (state <s> ^name move-to-goal
              ^pipe <p>
              ^mario <mario>
              ^rand-speed)
   (<p> ^distx <dx>
        ^height <height>
        ^isreachable yes)
   (<mario> ^xi <mx>
            ^yi <my>)
-->
   (<s> ^operator <o> +)
   (<o> ^name action
        ^direction right
        ^jump yes
        ^speed high)
}

sp {move*to*goal2
   :template
   (state <s> ^name move-to-goal
              ^operator <op> +
              ^pipe <p>
              ^mario <mario>)
   (<mario> ^type <mariot>
        ^tile-above <ta>)
   (<op> ^name action
         ^direction <d>
         ^jump <j>
         ^speed <sp>)
   (<p> ^distx <dx>
        ^height <height>
        ^isreachable yes)
--> 
   (<s> ^operator <op> = 0)
}

sp {move-to-goal*propose*action3
   (state <s> ^name move-to-goal
              ^tile-type <tt>)
  - (<tt> ^right <<|7| |\||>>)
-->
   (<s> ^operator <op> +)
   (<op> ^name action
         ^direction right
         ^jump no
         ^speed low)
}
