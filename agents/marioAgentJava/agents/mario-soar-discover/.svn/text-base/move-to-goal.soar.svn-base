sp {propose*move-to-goal
   (state <s> ^name mario-soar-discover
             ^mario <mario>)
-->
   (<s> ^operator <o> + <)
   (<o> ^name move-to-goal)
}
sp {move-to-goal*propose*action1
   (state <s> ^name move-to-goal
              ^superstate.mario <mario>)
     (<mario> ^xi <mx>
            ^yi <my>)
   (<mario> ^tile-on-right |7|)
-->
   (<s> ^operator <op> +)
   (<op> ^name action
         ^direction right
         ^jump yes
         ^speed low)
}
