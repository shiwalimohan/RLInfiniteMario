### add visible objects to to-learn
sp {mario-soar*propose*add-to-learn
   (state <s> ^name mario-soar
   	      ^object <obj>
	      ^to-learn <tl>)
   (<obj> ^visible true)
   (<tl> -^object <obj>)
-->
   (<s> ^operator <op> +)
   (<op> ^name add-to-learn
   	 ^object <obj>)
}
# elaborate add-to-learn to include all visible objects
sp {mario-soar*elaborate*add-to-learn	
    (state <s> ^name mario-soar
    	       ^object <obj>
	       ^operator <op> +)
   (<op> ^name add-to-learn)
   (<obj> ^visible true)
-->
   (<op> ^object <obj>)
}

sp {mario-soar*apply*add-to-learn
   (state <s> ^name mario-soar
   	      ^operator <op>
	      ^to-learn <tl>)
   (<op> ^object <obj>
   	 ^name add-to-learn)
-->
   (<tl> ^object <obj>)
}


### remove objects that are unvisible or donot exist
sp {mario-soar*propose*remove-learn
   (state <s> ^name mario-soar
   	      ^to-learn <tl>)
   (<tl> ^object <obj>)
 - {(<s> ^object <obj>)
    (<obj> ^visible true)}
-->
  (<s> ^operator <op> +)
  (<op> ^name remove-from-learn	
  	^object <obj>)
}

sp {mario-soar*elaborate*remove-learn
   (state <s> ^name mario-soar
   	      ^to-learn <tl>
	      ^operator <op> +)
   (<tl> ^object <obj>)
 - {(<s> ^object <obj>)
    (<obj> ^visible true)}
   (<op> ^name remove-from-learn)
-->
   (<op> ^object <obj>)
}

sp {mario-soar*apply*remove-learn
   (state <s> ^name mario-soar
   	      ^to-learn <tl>
	      ^operator <op>)
   (<tl> ^object <obj>)
   (<op> ^name remove-from-learn
   	 ^object <obj>)
-->
  (<tl> ^object <obj> -)
}



