sp {elaborate*future*movement
   (state <s> ^name tackle-monster
              ^monster.prediction <mp>)
   (<mp> ^position <mpp1>
         ^position {<mpp2> <> <mpp1>})
   (<mpp1> ^memory-id <id1>
           ^xd <x1>
           ^yd <y1>)
   (<mpp2> ^memory-id {<id2> > <id1>}
           ^xd <x2>
           ^yd <y2>)
-->
   (<mp> ^deltax (- <x2> <x1>)
         ^deltay (- <y2> <y1>))
}

sp {elaborate*predicted*position
   (state <s> ^name tackle-monster
              ^superstate <ss>
              ^monster <m>)
   (<m> ^prediction <mp>)
   (<mp> ^deltax <dx>
         ^deltay <dy>)
   (<m> ^xd <mx>
        ^yd <my>)
-->
   (<m> ^futurex (+ <mx> <dx>)
        ^futurey (+ <my> <dy>))
}
   