sp {default*selection*elaborate*name
   :default
   (state <s> ^impasse tie
   	      ^attribute operator)
   -->
   (<s> ^name selection)
}

sp {selection*propose*evaluate-operator
   :default
   (state <s> ^name selection
   	      ^item <i>)
 -{(state <s> ^evaluation <e>)
   (<e> ^operator <i>
       ^value true)}
   -->
  (<s> ^operator <o> +,=)
  (<o> ^name evaluate-operator
       ^operator <i>)
}
  
