sp {propose*random*action
   (state <s> ^name mario-soar-insight
              ^rand-move <mv>
              ^rand-jump <jp>
              ^coin <c>)
   (<c> ^isreachable yes)
   (<mv> ^direction <d>)
   (<jp> ^up <j>)
-->
   (<s> ^operator <o> +)
   (<o> ^name action
        ^move <d>
        ^jump <j>)
}
sp {grab*coin
   :template
   (state <s> ^operator <op> +
              ^coin <c>)
   (<c> ^distx <dx>
        ^disty <dy>
        ^isreachable yes)
   (<op> ^name action
         ^move <d>
         ^jump <j>)
--> 
   (<s> ^operator <op> = 0)
}
