sp {propose*tackle-monster
   (state <s> ^name mario-soar-hrlcomp
              ^monster <m>)
   (<m> ^isthreat yes)
  -(<m> ^isthreat no)
-->
   (<s> ^operator <o> +)
   (<o> ^name tackle-monster
        ^monster <m>)
}
gp {tackle*monster*general
  :template
   (state <s> ^name mario-soar-hrlcomp
              ^operator <op> +
              ^mario <Mario>)
   (<op> ^name tackle-monster
         ^monster <m>)
   (<m> ^distx <distx>
        ^disty <disty>
        ^sx <msx>
        ^sy <msy>
        ^winged <is>
        ^type <monst>)
   (<Mario> ^sx <mariosx>
            ^sy <mariosy>)
-->
   (<s> ^operator <op> = 0)
}
sp {monster*random*action
   (state <s> ^name tackle-monster
              ^rand-move <mv>
              ^rand-speed <sp>
              ^monster <m>                                                                                                                                                                              
              ^mario <mario>)
   (<m> ^distx <dx>
        ^disty <dy>
        ^direction <dir>)
   (<mv> ^up <j>) 
   (<mario> ^yi <my>)
   (<mv> ^direction <d>)
   (<sp> ^degree <sd>)
-->
   (<s> ^operator <o> +)
   (<o> ^name action
        ^direction <d>
        ^jump <j>
        ^speed <sd>)
}
sp {tackle*monster
   :template
   (state <s> ^name tackle-monster
              ^operator <op> +
              ^monster <m>
              ^mario <mario>)
   (<mario> ^type <mtype>
            ^tile-above <ta>)
   (<op> ^name action
         ^direction <dir>
         ^jump <jp>
         ^speed <sp>)
   (<m> ^type <monst>
        ^distx <dx>
        ^disty <dy>
        ^sx <sx>
        ^sy <sy>
        ^winged <isiwing>)
-->
   (<s> ^operator <op> = 0)
}
