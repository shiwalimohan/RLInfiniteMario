sp {propose*move-to-goal
   (state <s> ^io.input-link.mario <mario>
              ^tile-type.right <tr>)
   (<mario> ^xi <x>
            ^yi <y>)
-->
   (<s> ^operator <o> + <)
   (<o> ^name move-to-goal
        ^tile-right <tr>)
}
sp {move-to-goal*propose*action1
   (state <s> ^superstate.operator <o>)
   (<o> ^name move-to-goal
        ^tile-right |7|)
-->
