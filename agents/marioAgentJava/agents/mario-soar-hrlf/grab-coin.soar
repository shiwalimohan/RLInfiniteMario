sp {coin*random*action
   (state <s> ^name grab-coin
              ^rand-move <mv>
              ^rand-jump <jp>
              ^rand-speed <sp>
              ^mario <mario>)
   (<mv> ^direction <d>)
   (<jp> ^up <j>)
   (<mario> ^xi <mx>
            ^yi <my>)
   (<sp> ^degree <sd>)
-->
   (<s> ^operator <o> +)
   (<o> ^name action
        ^direction <d>
        ^jump <j>
        ^speed <sd>)
}
sp {grab*coin
   :template
   (state <s> ^name grab-coin
              ^operator <op> +
              ^coin <c>
              ^mario <m>)
   (<m> ^type <mt>
        ^tile-above <ta>)
   (<c> ^distx <dx>
        ^disty <dy>
        ^isreachable yes)
   (<op> ^name action
         ^direction <d>
         ^jump <j>
         ^speed <sd>)
--> 
   (<s> ^operator <op> = 0)
}