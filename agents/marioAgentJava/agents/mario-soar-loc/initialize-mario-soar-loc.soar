sp {propose*initialize-mario-soar-loc
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-mario-soar-loc)
}

sp {apply*initialize-mario-soar-loc
   (state <s> ^operator <op>)
   (<op> ^name initialize-mario-soar-loc)
-->
   (<s> ^name mario-soar-loc
        ^move <m1>
        ^move <m2>
        ^move <m3>
        ^jump <j1>
        ^jump <j2>)
   (<m1> ^direction left
         ^initial -1)
   (<m2> ^direction right
         ^initial 1)
   (<m3> ^direction nil
         ^initial 0)
   (<j1> ^up yes)
   (<j2> ^up no)
}

