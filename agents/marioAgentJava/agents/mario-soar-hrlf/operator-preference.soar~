sp {learn*top-level*flo1
   (state <s> ^name mario-soar-hrlf
              ^monster.isthreat yes
              ^coin.isreachable yes
              ^question.isreachable yes
              ^operator <o> +
              ^available-functions.function <f>)
   (<o> ^name <f>)
-->
   (<s> ^operator <o> = 0)
}
