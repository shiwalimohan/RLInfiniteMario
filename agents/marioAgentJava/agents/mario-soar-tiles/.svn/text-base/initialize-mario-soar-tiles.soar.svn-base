sp {propose*initialize-mario-soar-tiles
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-mario-soar-tiles)
}

sp {apply*initialize-mario-soar-tiles
   (state <s> ^operator <op>)
   (<op> ^name initialize-mario-soar-tiles)
-->
   (<s> ^name mario-soar-tiles
        ^move <m1>
        ^move <m2>
        ^move <m3>
        ^jump <j1>
        ^jump <j2>
        ^speed <s1>
        ^speed <s2>)
   (<m1> ^direction left
         ^initial -0)
   (<m2> ^direction right
         ^initial 1)
   (<m3> ^direction nil
         ^initial 0)
   (<j1> ^up yes)
   (<j2> ^up no)
   (<s1> ^degree low)
   (<s2> ^degree high)
}

