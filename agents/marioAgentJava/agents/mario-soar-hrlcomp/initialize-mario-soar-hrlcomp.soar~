sp {propose*initialize-mario-soar-hrlcomp
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-mario-soar-hrlcomp)
}

sp {apply*initialize-mario-soar-hrlcomp
   (state <s> ^operator <op>)
   (<op> ^name initialize-mario-soar-hrlcomp)
-->
   (<s> ^name mario-soar-hrlcomp
        ^rand-move <m1>
        ^rand-move <m2>
        ^rand-move <m3>
        ^rand-speed <s1>
        ^rand-speed <s2>)
   (<m1> ^direction left
         ^up yes
         ^up no)
   (<m2> ^direction right
         ^up yes
         ^up no)
   (<m3> ^direction nil
         ^up yes)
   (<s1> ^degree low)
   (<s2> ^degree high)
   (<s> ^metadata <m>)
}